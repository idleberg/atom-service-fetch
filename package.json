{
  "name": "service-fetch",
  "version": "0.3.7",
  "description": "Provides the fetch API through a web worker",
  "private": false,
  "main": "./lib/index.js",
  "typings": "./types",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-pattern .gitignore --no-warn-ignored",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run build",
    "start": "npm run dev",
    "test": "npm run lint",
    "prepare": "husky"
  },
  "keywords": [
    "fetch",
    "fetch api",
    "http request",
    "web worker",
    "web workers",
    "xmlhttprequest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-service-fetch"
  },
  "homepage": "https://atom.io/packages/service-fetch",
  "bugs": {
    "url": "https://github.com/idleberg/atom-service-fetch/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.56.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "service-fetch": {
      "description": "Provides the fetch API through a web worker",
      "versions": {
        "0.2.0": "provideFetch"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.6.1",
    "@stylelint/remark-preset": "^5.1.1",
    "@types/atom": "^1.40.18",
    "@types/node": "^14.18.63",
    "cron-parser": "^3.5.0",
    "cross-fetch": "^3.1.8",
    "eslint": "^9.12.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "hash-wasm": "^4.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "log-symbols": "^5.1.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "remark-cli": "^12.0.1",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "uuid": "^8.3.2"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "lint-staged": {
    "*.(json|ts)": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
